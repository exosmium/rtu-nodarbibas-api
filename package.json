{
  "name": "rtu-nodarbibas-api",
  "version": "1.0.1",
  "description": "NPM package for scraping RTU schedule data from nodarbibas.rtu.lv",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "rtu-nodarbibas-api": "dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.js --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run lint && npm run type-check && npm run test:run && npm run build"
  },
  "keywords": [
    "rtu",
    "schedule",
    "scraper",
    "nodarbibas",
    "university",
    "timetable"
  ],
  "author": "exosmium",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/exosmium/rtu-nodarbibas-api.git"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^11.0.0",
    "dayjs": "^1.11.10"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=16"
  }
}
